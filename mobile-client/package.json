{
  "name": "mobile-client",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "start:dev": "expo start --dev-client",
    "start:dev:tunnel": "expo start --dev-client --tunnel",
    "start:prod": "EXPO_PUBLIC_ENV=production expo start --no-dev --minify",
    "start:prod:tunnel": "EXPO_PUBLIC_ENV=production expo start --no-dev --minify --tunnel",
    "build:dev": "expo build",
    "build:prod": "expo build --config app.production.json",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "android:tunnel": "expo start --android --tunnel",
    "android:dev": "expo start --android --dev-client",
    "android:dev:tunnel": "expo start --android --dev-client --tunnel",
    "android:prod": "EXPO_PUBLIC_ENV=production expo start --android --no-dev --minify",
    "android:prod:tunnel": "EXPO_PUBLIC_ENV=production expo start --android --no-dev --minify --tunnel",
    "ios": "expo start --ios",
    "ios:tunnel": "expo start --ios --tunnel",
    "ios:dev": "expo start --ios --dev-client",
    "ios:dev:tunnel": "expo start --ios --dev-client --tunnel", 
    "ios:prod": "EXPO_PUBLIC_ENV=production expo start --ios --no-dev --minify",
    "ios:prod:tunnel": "EXPO_PUBLIC_ENV=production expo start --ios --no-dev --minify --tunnel",
    "web": "expo start --web",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "54.0.12",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.9",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
